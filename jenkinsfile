pipeline{
  agent any
  stages{
    stage("build"){
      steps{
        echo 'Construyendo la aplicación...'
      }
    }
    stage("test"){
      steps{
        echo 'Testeando la aplicación...'
      }
    }
    stage("deploy"){
      steps{
        echo 'Deployando la aplicación...'
      }
    }
  }
}
